{
  "name": "typescript-react-webpack-template",
  "version": "1.0.0",
  "description": "A starter template for React Project with TypeScript, Webpack, SWC, Jest, Eslint, and Prettier.",
  "license": "MIT",
  "private": true,
  "keywords": [
    "typescript",
    "swc",
    "webpack",
    "react"
  ],
  "author": "nyi nyi thann",
  "devDependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "@svgr/webpack": "^6.3.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.245",
    "@swc/helpers": "^0.4.11",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "sass": "^1.54.7",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.11",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1",
    "webpack-merge": "^5.8.0",
    "yarn-upgrade-all": "^0.7.1"
  },
  "dependencies": {
    "modern-normalize": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "dashboard": "webpack-dashboard -- webpack serve --config build/webpack.js --env env=dev addon=dashboard",
    "lint": "npx eslint --config .eslintrc.js 'src/*.tsx' 'src/**/*.tsx'",
    "lint:fix": "yarn lint --fix",
    "prettier": "npx prettier --config .prettierrc 'src/*.tsx' 'src/**/*.tsx' --check",
    "prettier:fix": "yarn prettier --write",
    "format": "yarn prettier:fix && npm run lint:fix",
    "stylelint": "stylelint --config .stylelintrc.json --fix 'style/*.css' 'src/*.css' 'src/**/*.css' ",
    "build:prod": "NODE_ENV=production && webpack --config build/webpack.js --env env=prod",
    "build:dev": "NODE_ENV=development && webpack --config build/webpack.js --env env=dev",
    "start": "NODE_ENV=development webpack serve --config build/webpack.js --env env=dev"
  }
}
